#ifndef _VARARG_H_#define _VARARG_H_/* * VarArgs.h -- Portable variable-argument list declarations */#ifndef __va_list__#define __va_list__typedef char *va_list;#endif#ifdef va_dcl#undef va_dcl#endif#define va_dcl char *va_alist;#ifdef va_start#undef va_start#endif#define va_start(list) list = (char *) &va_alist#ifdef va_end#undef va_end#endif#define va_end(list)#ifdef va_arg#undef va_arg#endif#define va_arg(list, mode) ((mode *)(list += sizeof(mode)))[-1]#endif _VARARG_H_